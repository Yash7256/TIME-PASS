{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/gallery",
      "dest": "/index-gallery.html"
    },
    {
      "src": "/landing",
      "dest": "/index-landing.html"
    }
  ]
}